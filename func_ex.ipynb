    "def myfunc(word_A):\n",
    "    l = len(word_A)\n",
    "    new_word = ''\n",
    "    for i in range(l):\n",
    "        if i % 2 == 0:\n",
    "            new_word += (word_A[i].upper())\n",
    "        else:\n",
    "            new_word += (word_A[i].lower())\n",
    "    return new_word\n",
    "myfunc('ciesietuwyrabiaX')"
